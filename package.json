    "peerDependencies": {
        "solid-js": ">=1.0.0<2"
    },
    "devDependencies": {
        "@babel/core": "^7.18.9",
        "@jest/types": "^28.1.3",
        "@size-limit/preset-small-lib": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "babel-preset-solid": "^1.4.8",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "jest": "^28.1.3",
        "pinst": "^3.0.0",
        "prettier": "^2.7.1",
        "size-limit": "^8.0.0",
        "ts-jest": "^28.0.7",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "solid-js": "^1.4.8"
