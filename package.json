{
    "name": "solid-swipe-card",
    "description": "A SolidJS swipeable card component (tinder-like)",
    "author": "mardisen",
    "repository": "mardisen/solid-swipe-card",
    "homepage": "https://solid-swipe-card-mardisen.vercel.app/",
    "version": "0.2.2",
    "license": "MIT",
    "private": false,
    "keywords": [
        "solid-js",
        "solid",
        "tinder",
        "card",
        "web",
        "swipeable",
        "swipe",
        "physics",
        "deck",
        "drag",
        "draggable"
    ],
    "main": "dist/index.d.ts",
    "module": "dist/solid-swipe-card.esm.js",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
