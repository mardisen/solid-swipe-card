{
    "name": "solid-swipe-card",
    "description": "A SolidJS swipeable card component (tinder-like)",
    "author": "mardisen",
    "repository": "mardisen/solid-swipe-card",
    "homepage": "https://solid-swipe-card-mardisen.vercel.app/",
    "version": "0.2.3",
    "license": "MIT",
    "private": false,
    "keywords": [
        "solid-js",
        "solid",
        "tinder",
        "card",
        "web",
        "swipeable",
        "swipe",
        "physics",
        "deck",
        "drag",
        "draggable"
    ],
    "main": "dist/index.d.ts",
    "module": "dist/index.js",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsc -w",
        "build": "tsc",
        "test": "jest",
        "coverage": "jest --coverage",
        "test-watch": "jest --watch",
        "lint": "eslint src/**",
        "prepare": "tsc",
        "size": "size-limit",
        "postinstall": "husky install",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable",
        "storybook": "yarn build && cd stories && yarn install && yarn storybook",
        "silent-storybook": "yarn build && cd stories && yarn install && yarn storybook --ci",
        "build-storybook": "yarn build && cd stories && yarn install && yarn build-storybook",
        "changelog": "github_changelog_generator -u mardisen -p solid-swipe-card"
    },
    "peerDependencies": {
        "solid-js": ">=1.0.0<2",
        "tslib": ">=2.0.0<3"
    },
    "devDependencies": {
        "@babel/core": "^7.18.9",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-typescript": "^7.18.6",
        "@jest/types": "^28.1.3",
        "@size-limit/preset-small-lib": "^8.0.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@types/jest": "^28.1.6",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "babel-jest": "^28.1.3",
        "babel-preset-solid": "^1.4.8",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "jsdom": "^20.0.0",
        "pinst": "^3.0.0",
        "prettier": "^2.7.1",
        "regenerator-runtime": "^0.13.9",
        "size-limit": "^8.0.0",
        "solid-jest": "^0.2.0",
        "solid-testing-library": "^0.3.0",
        "ts-jest": "^28.0.7",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "solid-js": "^1.4.8",
        "tslib": "^2.4.0"
    }
}
